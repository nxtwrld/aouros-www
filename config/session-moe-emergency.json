{
  "name": "Emergency Medicine Analysis",
  "description": "Rapid assessment focused on critical conditions",
  "targetAudience": "emergency_physicians",
  "complexity": "specialized",
  "experts": ["emergency_triage", "rapid_diagnosis", "safety_monitor"],
  "consensus": {
    "algorithm": "weighted_voting",
    "expertWeights": {
      "emergency_triage": 1.0,
      "rapid_diagnosis": 0.9,
      "safety_monitor": 1.2
    },
    "settings": {
      "minimumAgreement": 0.4,
      "conflictThreshold": 0.5,
      "requireMajorityForCritical": true,
      "uncertaintyThreshold": 0.7
    }
  }
}
